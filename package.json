  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheWitcher1991/ecjs"
  },
  "engines": {
    "node": ">= 14.x",
    "npm": ">= 7.x"
  },
  "devDependencies": {
    "electron-updater": "^4.3.9",
    "exceljs": "^4.3.0",
    "xlsx": "^0.17.1",
    "xlsx-populate": "^1.21.0"
  },
  "dependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "asar": "^3.0.3",
    "devtron": "^1.4.0",
    "electron": "^7.1.7",
    "electron-log": "^4.1.1",
    "electron-packager": "^14.1.1",
    "electron-winstaller": "^4.0.0",
    "eslint": "^7.32.0",
    "npm-run-all": "^4.1.5"
