{
  "name": "ecjs",
  "version": "1.0.0",
  "main": "./main.js",
  "bin": "./cli.js",
  "private": true,
  "scripts": {
    "start": "electron .",
    "dev": "electron . --debug",
    "build": "webpack --mode production",
    "watch": "webpack --mode production -w",
    "package": "npm-run-all package:*",
    "package:win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --out=build --icon=",
    "package:linux": "electron-packager . --overwrite --platform=linux --arch=x64 --out=build"
  },
  "author": "GitHub: TheWitcher1991",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheWitcher1991/ecjs"
  },
  "engines": {
    "node": ">= 14.x",
    "npm": ">= 7.x"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-transform-for-of": "^7.4.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/runtime": "^7.6.3",
    "@symfony/webpack-encore": "^1.6.1",
    "asar": "^3.0.3",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "devtron": "^1.4.0",
    "electron": "^7.1.7",
    "electron-log": "^4.1.1",
    "electron-packager": "^14.1.1",
    "electron-updater": "^4.3.9",
    "electron-winstaller": "^4.0.0",
    "eslint": "^6.5.1",
    "exceljs": "^4.3.0",
    "express": "^4.17.1",
    "file-saver": "^2.0.5",
    "npm-run-all": "^4.1.5",
    "sheetjs-style": "^0.15.7",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "xlsx": "^0.17.3",
    "xlsx-populate": "^1.21.0",
    "xlsx-pro": "^0.14.5"
  }
}
